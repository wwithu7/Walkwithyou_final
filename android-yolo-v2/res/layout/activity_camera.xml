<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fbackground"
    tools:context="org.tensorflow.wwithu72.view.CameraActivity">


    <RelativeLayout
        android:id="@+id/ctoolbar"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@color/toolbar"
        android:layout_alignParentTop="true">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/toolbar"
            />
        <Button
            android:id="@+id/cam_sett"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:background="@drawable/settingwhite"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/cbuttonbar"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:layout_marginTop="44dp"
        android:background="@color/btnColor">

        <Button
            android:id="@+id/btn_map"
            android:layout_width="120dp"
            android:layout_height="64dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:background="@drawable/mapbutton"
            android:onClick="onMapClick"
            android:accessibilityTraversalAfter="@id/cam_sett"/>

        <Button
            android:id="@+id/cam_cam"
            android:layout_width="120dp"
            android:layout_height="64dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/cameracolorbutton"
            android:accessibilityTraversalAfter="@id/btn_map"/>

        <Button
            android:id="@+id/btn_bus"
            android:layout_width="120dp"
            android:layout_height="64dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:background="@drawable/busbutton"
            android:onClick="onBusClick"
            android:accessibilityTraversalAfter="@id/cam_cam"/>


    </RelativeLayout>

